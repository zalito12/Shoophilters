{
	"name": "shoophilters",
	"version": "0.2.0",
	"description": "Shoophilters blocks to extend default woocomerce filter fields.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"scripts": {
		"build": "wp-scripts build --webpack-copy-php --output-path=blocks/build/ --webpack-src-dir=blocks/src",
		"start": "wp-scripts start --webpack-copy-php --output-path=blocks/build/ --webpack-src-dir=blocks/src",
		"build:order": "wp-scripts build --webpack-copy-php --output-path=blocks/build/product-orderby --webpack-src-dir=blocks/src/product-orderby",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip"
	},
	"devDependencies": {
		"@wordpress/scripts": "^26.8.0"
	},
	"dependencies": {
		"@wordpress/core-data": "^6.14.0"
	}
}
