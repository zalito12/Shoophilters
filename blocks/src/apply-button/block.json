{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "shoophilters/apply-button",
	"version": "0.1.0",
	"title": "Shoophilters: Apply filters button",
	"category": "shoophilters",
	"icon": "button",
	"description": "Add attribute filters for your shop",
	"keywords": [ "Shoophilters", "WooCommerce", "Filter", "Button" ],
	"supports": {
		"html": false
	},
	"textdomain": "shoophilters",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"attributes": {
		"group": {
			"type": "string",
			"default": "default"
		},
		"type": {
			"enum": ["button", "text"],
			"default": "button"
		},
		"text": {
			"type": "string",
			"default": "Apply Filters"
		}
	}
}
