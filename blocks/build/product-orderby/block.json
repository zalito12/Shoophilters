{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "shoophilters/product-orderby",
  "version": "0.1.0",
  "title": "Shoophilters: Order by filter",
  "category": "shoophilters",
  "icon": "randomize",
  "description": "Add an order by filter",
  "keywords": [
    "Shoophilters",
    "WooCommerce",
    "Filter",
    "Order"
  ],
  "supports": {
    "html": false
  },
  "textdomain": "shoophilters",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:render.php",
  "attributes": {
    "type": {
      "enum": [
        "list",
        "select"
      ],
      "default": "list"
    },
    "showAlways": {
      "type": "boolean",
      "default": "true"
    },
    "filtering": {
      "type": "object",
      "default": {
        "navigation": "standard",
        "group": "default"
      }
    }
  }
}