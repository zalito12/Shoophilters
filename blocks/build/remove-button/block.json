{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "shoophilters/remove-button",
  "version": "0.1.0",
  "title": "Shoophilters: Remove filters button",
  "category": "shoophilters",
  "icon": "button",
  "description": "Add attribute filters for your shop",
  "keywords": [
    "Shoophilters",
    "WooCommerce",
    "Filter",
    "Button"
  ],
  "supports": {
    "html": false
  },
  "textdomain": "shoophilters",
  "editorScript": "file:./index.js",
  "attributes": {
    "group": {
      "type": "string",
      "default": "default"
    },
    "type": {
      "enum": [
        "button",
        "text"
      ],
      "default": "button"
    },
    "text": {
      "type": "string",
      "default": "Reset Filters"
    }
  }
}